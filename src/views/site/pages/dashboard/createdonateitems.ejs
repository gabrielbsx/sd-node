<div class="col-md-8 p-0 m-0">
    <div class="col-md-12 my-2">
        <div class="col-md-12 py-3 bg-warning text-dark text-uppercase font-weight-bold">
            Painel de controle - criar itens de doações
        </div>
        <div style="background-color: rgba(0,0,0,0.5);" class="text-light py-4 px-5">
            <form method="post" action="/painel-de-controle/adicionar-itens-de-doacoes">
                <div class="form-group">
                    <input name="itemname" type="text" class="form-control" placeholder="Nome do item">
                </div>
                <div class="form-group">
                    <input name="slug" type="text" class="form-control" placeholder="slug">
                </div>
                <div class="form-group">
                    <input name="item_id" type="text" class="form-control" placeholder="item_id">
                </div>
                <div class="form-group">
                    <input name="eff1" type="text" class="form-control" placeholder="eff1">
                </div>
                <div class="form-group">
                    <input name="effv1" type="text" class="form-control" placeholder="effv1">
                </div>
                <div class="form-group">
                    <input name="eff2" type="text" class="form-control" placeholder="eff2">
                </div>
                <div class="form-group">
                    <input name="effv2" type="text" class="form-control" placeholder="effv2">
                </div>
                <div class="form-group">
                    <input name="eff3" type="text" class="form-control" placeholder="eff3">
                </div>
                <div class="form-group">
                    <input name="effv3" type="text" class="form-control" placeholder="effv3">
                </div>
                <div class="form-group">
                    <select name="id_package" class="form-control">
                        <% for (let i = 0; i < data.length; i++) { %>
                            <option value="<%= data[i].id %>"><%= data[i].name %></option>
                        <% } %>
                    </select>
                </div>
                <input type="hidden" class="g-recaptcha-response" name="g-recaptcha-response">
                <input type="hidden" name="action" value="validate_captcha">
                <div class="form-group">
                    <button class="btn btn-primary">
                        Criar
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>